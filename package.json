{
  "name": "localhost-tunnel",
  "version": "1.0.0",
  "description": "A simple web browser based app to make local http server live on the internet",
  "keywords": [
    "localhost",
    "tunnel",
    "easy",
    "online",
    "proxy"
  ],
  "author": {
    "name": "sheikhMishar00",
    "email": "misharex.sezan@gmail.com"
  },
  "homepage": "https://github.com/sheikhmishar/localhost-tunnel",
  "bugs": {
    "url": "https://github.com/sheikhmishar/localhost-tunnel/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sheikhmishar/localhost-tunnel.git"
  },
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "test": "nodemon -x \"npm run test:server && npm run test:client\"",
    "test:server": "mocha \"./{,!(views|node_modules)/**}/*.test.js\"",
    "test:client": "rundll32 url.dll,FileProtocolHandler http://localhost:5000/test && npm run watch:test",
    "test:servern": "mocha \"./@(controllers|models|helpers)/**/*.test.js\"",
    "build": "npm run build:heroku --prefix views",
    "build:dev": "npm run build:dev --prefix views",
    "postinstall": "npm i --prefix views",
    "watch": "concurrently \"nodemon -i views\" \"npm run watch --prefix views\"",
    "watch:debug": "nodemon -e js,json -x \"node --inspect-brk server.js\""
  },
  "engines": {
    "node": "v12.16.1",
    "npm": "6.13.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0",
    "uuid": "^8.2.0"
  },
  "optionalDependencies": {
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/multer": "^1.4.4",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.33"
  },
  "devDependencies": {
    "mocha": "^8.1.3"
  }
}
